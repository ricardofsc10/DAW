extends layout
block content  
    .w3-container
        .w3-card-4
            header.w3-container.w3-teal
                h3 Lista de musicas do arquivo sonoro
                a(href='/') Voltar à página principal
            .w3-container
                table.w3-table-all
                    tr
                        th Província
                        th Local
                        th Título
                        th Músico
                        th Duracao
                    each mu in lista
                        tr
                            td=mu.prov
                            td=mu.local
                            td=mu.tit
                            td=mu.musico
                            td=mu.duracao
            footer.w3-container.w3-teal
                address Arquivo Son EVO App